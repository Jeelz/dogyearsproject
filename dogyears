const myAge = 32;
let earlyYears = 2;
earlyYears *= 10.5
laterYears = myAge - 2;
laterYears *= 4;
let myAgeInDogYears = earlyYears + laterYears;
console.log(myAgeInDogYears);
myName = 'Jill'
myName.toLowerCase();
console.log(`My name is ${myName}, I am ${myAge} old in human years which is ${myAgeInDogYears} in dog years.`)
